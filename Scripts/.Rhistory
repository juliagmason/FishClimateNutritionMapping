# manually add country codes; filter out howland and virgin islands (not in AMC)
codes_AMC <- codes %>%
filter (!iso3_terr %in% c("USA-VI", "USA-HUN")) %>%
select (-Country)
AMC <- cbind (AMC, codes_AMC)
